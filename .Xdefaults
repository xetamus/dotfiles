URxvt*font: xft:Cascadia Mono PL:size=12
URxvt*boldFont: xft:Cascadia Mono PL
URxvt.letterSpace: 0

! special
*.background: #0c0c0c
*.foreground: #d6d6d6
*.cursorColor: #009688
urxvt*depth: 32
urxvt*background: rgba:00c0/00c0/00c0/e000

! black
*.color0:       #2a2e32
*.color8:       #31363b

! red
*.color1:       #c0392b
*.color9:       #f44f4f

! green
*.color2:       #218058
*.color10:      #9fca56

! yellow
*.color3:       #fdbc4b
*.color11:      #fdbc4b

! blue
*.color4:       #2980b9
*.color12:      #0099ff

! magenta
*.color5:       #8e44ad
*.color13:      #af81ff

! cyan
*.color6:       #27aeae
*.color14:      #31dddd

! white
*.color7:       #acada1
*.color15:      #cfd0c2


#ifdef DARWIN_ENV
URxvt.perl-lib: ~/lib/urxvt/perl
URxvt.perl-ext-common: macosx-clipboard
URxvt*keysym.M-c: perl:macosx-clipboard:copy
URxvt*keysym.M-v: perl:macosx-clipboard:paste
#endif

URxvt.perl-ext-common:  ...,clipboard,...
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

URxvt.termName: xterm-256color
URxvt.loginShell: True

URxvt.skipScroll: False
URxvt.scrollstyle: plain
URxvt.scrollBar: False
URxvt.scrollTtyOutput: False
URxvt.scrollTtyKeypress: True
URxvt.scrollWithBuffer: True

URxvt.saveLines: 100000

URxvt.backspaceKey: Backspace
URxvt.deleteKey: Delete
